{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "core-js": "^2.6.5",
    "datatables.net": "^1.10.19",
    "datatables.net-dt": "^1.10.19",
    "datatables.net-responsive": "^2.2.3",
    "flatpickr": "^4.6.2",
    "gsap": "^2.1.3",
    "jquery": "^3.4.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "shards-vue": "^1.0.7",
    "vee-validate": "^2.2.11",
    "vue": "^2.6.10",
    "vue-carousel": "^0.18.0",
    "vue-chartjs": "^3.4.2",
    "vue-flatpickr-component": "^8.1.3",
    "vue-izitoast": "^1.2.1",
    "vue-json-excel": "^0.2.98",
    "vue-router": "^3.0.3",
    "vue-select": "^3.1.0",
    "vue2-google-maps": "^0.10.7",
    "vuex": "^3.0.1",
    "webpack": "^4.39.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "no-shadow": [
        "error",
        {
          "allow": [
            "state"
          ]
        }
      ],
      "prefer-destructuring": [
        "error",
        {
          "object": true,
          "array": false
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "$": true,
      "jQuery": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
